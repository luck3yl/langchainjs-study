#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules/openai/bin/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules/openai/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/node_modules:/Users/yyl/Documents/yyl/study/node_modules:/Users/yyl/Documents/yyl/node_modules:/Users/yyl/Documents/node_modules:/Users/yyl/node_modules:/Users/node_modules:/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules/openai/bin/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules/openai/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/node_modules:/Users/yyl/Documents/yyl/study/node_modules:/Users/yyl/Documents/yyl/node_modules:/Users/yyl/Documents/node_modules:/Users/yyl/node_modules:/Users/node_modules:/node_modules:/Users/yyl/Documents/yyl/study/yyl-ai/jupyter-ai/js/node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
